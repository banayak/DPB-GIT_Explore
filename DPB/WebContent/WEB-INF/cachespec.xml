<?xml version="1.0" ?>
<!DOCTYPE cache SYSTEM "cachespec.dtd">
<cache>
	<cache-instance name = "services/cache/bpmCache">
	<cache-entry>
		<class>command</class>
		<sharing-policy>not-shared</sharing-policy>
		<name>com.mbusa.dpb.common.util.CacheManager.class</name>
		<cache-id>
			<component type="method" id="getKpiId">
				<required>true</required>
			</component>
			<priority>1</priority>
			<timeout>3600</timeout>
		</cache-id>
	</cache-entry>
	</cache-instance>
</cache>